function getIndexToIns(arr, num) {
  // Find my place in this sorted array.
  arr.sort((a, b) => a-b);  
  if (num > arr[arr.length-1]) return arr.length; 
  for (let i = 0; i < arr.length; i++) {
    if (num <= arr[i]) return i;        
  } 
  return 0;
}

console.log(getIndexToIns([2, 5, 10], 15));
